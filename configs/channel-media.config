name: {{ project_name }}
rootdir: {{ project_rootdir }}
links:
  - name: channel2
    location: mhp/channel2/java
  - name: media2
    location: mhp/media2/java
classpaths:
  - kind: con
    path: org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/JavaSE-1.4
  - kind: output
    path: bin
  - kind: src
    path: src
  - kind: lib
    path: build/{{ build_target }}/jvmbtclasses.zip
  - kind: lib
    path: build/{{ build_target }}/classes.zip
  - kind: lib
    path: build/{{ build_target }}/classes
  - kind: src
    path: media2
    excluding: mhp10002/org/dvb/media/
  - kind: src
    path: channel2

